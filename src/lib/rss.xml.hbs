<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
<channel>
  <title>{{ title }}</title>
  <link>{{ link }}</link>
  <description>{{ description }}</description>
	{{#each items}}
		<item>
			<title>{{ this.title }}</title>
			<link>{{ this.link}}</link>
			<pubDate>{{ this.pubDate }}</pubDate>
			<description>{{ this.description }}</description>
		</item>
	{{/each}}
</channel>
</rss>